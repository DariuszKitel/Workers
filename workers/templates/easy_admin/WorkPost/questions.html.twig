<table class="table table-striped table-bordered">
    <thead>
    <tr>
        <th>ID</th>
        <th>Author</th>
        <th>Content</th>
    </tr>
    </thead>
    {% for question in value %}
        <tr>
            <td>{{ question.id }}</td>
            <td>{{ question.author.name }}</td>
            <td>{{ question.content }}</td>
        </tr>
    {% endfor %}
</table>